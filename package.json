    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "git-cz",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:eslint": "eslint \"src/**/*.{vue,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint  src/**/*.{vue,scss,css,sass,less} --fix",
